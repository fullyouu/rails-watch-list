<div class="content">
  <div class="cards">
  <% @lists.each do |list| %>
    <%= link_to list_path(list) do %>
    <div class="card">
      <%= cl_image_tag list.photo.key, crop: :fill, class: "list-image" %>
      <div class="card-body">
        <p class="card-text"><%= list.name %></p>
      </div>
    </div>
    <% end %>
  <% end %>
  </div>
</div>

<% @lists.each do |list| %>
  <div class="card-trip">
    <%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill %>
    <div class="card-trip-infos">
      <div>
        <p><%= link_to list.name, list_path(list) %></p>
      </div>
      <h2 class="card-trip-pricing">Rating</h2>
    </div>
  </div>
<% end %>
